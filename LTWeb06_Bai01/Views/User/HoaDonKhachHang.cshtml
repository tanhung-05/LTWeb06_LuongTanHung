@model IEnumerable<LTWeb06_Bai01.Models.HoaDon>

@{
    ViewBag.Title = "HoaDonKhachHang";
}

<h2>HoaDonKhachHang</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaHoaDon)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NgayTao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaKH)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaHoaDon)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NgayTao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaKH)
        </td>
        <td>
            @Html.ActionLink("Chi tiết", "CTHoaDon", new { id = item.MaHoaDon.Trim() }, new {@class = "nav-link"})
        </td>
    </tr>
}

</table>
